type storage = list<nat>;

type request = {
   owner: address,
   token_id: nat
};

type callback = {
   request: request,
   balance: nat
};

type parameter = list<callback>;

type return_type = [list<operation>, storage];

const main = (responses: parameter, _storage: storage): return_type => {
  const balances: list<nat> = List.map((r: callback): nat => r.balance, responses);

  return [list([]), balances];
};
